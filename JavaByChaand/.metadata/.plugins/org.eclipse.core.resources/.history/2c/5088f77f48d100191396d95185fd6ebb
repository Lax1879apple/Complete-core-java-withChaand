
public class SwitchStatements {

	public static void main(String[] args) {
		int x = 1;
		
		switch (x) {
		case 1: 
			System.out.println("X is greater than 8");
				break;
		case 2: 
			System.out.println("X is equal to 8");
				break;
		case 3: 
			System.out.println("X is smaller than 8");
				break;
		default: 
			System.out.println("X is greater than 8");
				break;
			
		}

	}

}
